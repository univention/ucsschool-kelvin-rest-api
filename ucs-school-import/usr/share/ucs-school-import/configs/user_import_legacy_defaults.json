{
	"factory": "ucsschool.importer.legacy.legacy_csv_user_import_factory.LegacyCsvUserImportFactory",
	"classes": {},
	"input": {
		"type": "csv",
		"filename": "/var/lib/ucs-school-import/legacy-format-userimport.csv"
	},
	"csv": {
		"delimiter": "\t",
		"header_lines": 0,
		"mapping": {
			"0": "__action",
			"1": "name",
			"2": "lastname",
			"3": "firstname",
			"4": "school",
			"5": "school_classes",
			"6": "__ignore",
			"7": "email",
			"8": "__is_teacher",
			"9": "__activate",
			"10": "__is_staff",
			"11": "password",
			"12": "__ignore",
			"13": "__ignore",
			"14": "__ignore",
			"15": "__ignore",
			"16": "__ignore",
			"17": "__ignore",
			"18": "__ignore",
			"19": "__ignore",
			"20": "__ignore",
			"21": "__ignore",
			"22": "__ignore",
			"23": "__ignore",
			"24": "__ignore",
			"25": "__ignore",
			"26": "__ignore",
			"27": "__ignore",
			"28": "__ignore",
			"29": "__ignore",
			"30": "__ignore"
		}
	},
	"scheme": {
		"email": "<firstname>[0].<lastname>@<maildomain>",
		"recordUID": "<name>",
		"username": {
			"default": "<:umlauts><firstname>[0].<lastname><:lower>[COUNTER2]"
		}
	},
	"no_delete": false,
	"password_length": 8,
	"sourceUID": "LegacyDB",
	"tolerate_errors": -1
}
