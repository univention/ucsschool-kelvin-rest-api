{
	"factory": "ucsschool.importer.default_factory.DefaultFactory",
	"input": {
		"type": null
	},
	"activate_new_users": {
		"default": true
	},
	"scheme": {
		"email": "<firstname>[0].<lastname>[COUNTER2]@<maildomain>",
		"rid": "<username>",
		"username": {
			"default": "<:umlauts><firstname>[0].<lastname>[COUNTER2]"
		}
	},
	"csv_output": "/tmp/out.csv",
	"dry_run": true,
	"logfile": "/var/log/univention/ucsschool-import.log",
	"maildomain": null,
	"mandatory_attributes": ["firstname", "lastname", "name", "school"],
	"no_delete": false,
	"outdated_users": {
		"delete": true,
		"deactivate": false,
		"waiting_period": 0
	},
	"output": {
		"passwords": "/var/lib/ucs-school-import/new_user_passwords.csv"
	},
	"password_length": 15,
	"school": null,
	"sourceUID": null,
	"tolerate_errors": 0,
	"user_deletion": {
		"delete": true,
		"expiration": 0
	},
	"user_role": null,
	"verbose": true
}
