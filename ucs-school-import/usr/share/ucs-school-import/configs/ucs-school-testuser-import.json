{
	"factory": "ucsschool.importer.default_user_import_factory.DefaultUserImportFactory",
	"classes": {
		"reader": "ucsschool.importer.reader.test_csv_reader.TestCsvReader"
	},
	"input": {
		"type": "csv"
	},
	"csv": {
		"mapping": {
			"Schulen": "schools",
			"Benutzertyp": "__type",
			"Vorname": "firstname",
			"Nachname": "lastname",
			"Klassen": "school_classes",
			"Beschreibung": "description",
			"Telefon": "phone"
		}
	},
	"scheme": {
		"email": "<firstname>.<lastname>@<maildomain>",
		"recordUID": "<firstname>.<lastname>",
		"username": {
		    "default": "<:umlauts><firstname>[0].<lastname><:lower>[COUNTER2]",
		    "student": "<:umlauts><firstname><:lower>[COUNTER2]"
		}
	},
	"sourceUID": "TESTID",
	"verbose": false
}
