{
	"factory": "ucsschool.importer.default_user_import_factory.DefaultUserImportFactory",
	"classes": {},
	"input": {
		"type": "csv",
		"filename": "/var/lib/ucs-school-import/new-format-userimport.csv"
	},
	"csv": {
		"header_lines": 1,
		"incell-delimiter": {
			"default": ","
		},
		"mapping": {}
	},
	"activate_new_users": {
		"default": true
	},
	"scheme": {
		"email": "<firstname>[0].<lastname>[COUNTER2]@<maildomain>",
		"rid": "<email>",
		"username": {
			"allow_rename": false,
			"default": "<:umlauts><firstname>[0].<lastname>[COUNTER2]"
		}
	},
	"maildomain": null,
	"mandatory_attributes": ["firstname", "lastname", "name", "school"],
	"no_delete": false,
	"outdated_users": {
		"delete": true,
		"deactivate": false,
		"waiting_period": 0
	},
	"output": {
		"new_user_passwords": "/var/lib/ucs-school-import/new_user_passwords.csv",
		"user_import_summary": "/var/lib/ucs-school-import/user_import_summary.csv"
	},
	"password_length": 15,
	"school": null,
	"sourceUID": null,
	"tolerate_errors": 0,
	"user_deletion": {
		"delete": true,
		"expiration": 0
	},
	"user_role": null
}
