{
	"factory": "ucsschool.importer.default_user_import_factory.DefaultUserImportFactory",
	"classes": {},
	"configuration_checks": ["defaults"],
	"input": {
		"type": "csv",
		"filename": ""
	},
	"csv": {
		"allowed_missing_columns": [],
		"header_lines": 1,
		"incell-delimiter": {
			"default": ","
		},
		"mapping": {}
	},
	"activate_new_users": {
		"default": true
	},
	"deletion_grace_period": {
		"deactivation": 0,
		"deletion": 0
	},
	"scheme": {},
	"maildomain": null,
	"mandatory_attributes": ["firstname", "lastname", "name", "record_uid", "school", "source_uid"],
	"no_delete": false,
	"output": {
		"new_user_passwords": null,
		"user_import_summary": "/var/lib/ucs-school-import/summary/%Y/%m/user_import_summary_%Y-%m-%d_%H:%M:%S.csv"
	},
	"password_length": 15,
	"school": null,
	"source_uid": null,
	"tolerate_errors": 0,
	"user_role": null,
	"username": {
		"allowed_special_chars": "."
	},
	"normalize": {
		"firstname": false,
		"lastname": false
	},
	"school_classes_invalid_character_replacement": "-",
	"school_classes_keep_if_empty": false
}
